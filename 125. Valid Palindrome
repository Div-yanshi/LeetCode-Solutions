class Solution {
    public boolean isPalindrome(String s) {
   s=s.trim();
   int l=s.length();
   s=s.toLowerCase();
   String str1="", str2="";
   for(int i=0;i<l;i++)
   {
    char c=s.charAt(i);
    if((c>='a'&& c<='z')  || (c>='0' && c<='9')){
    str1=str1+c;
   }}
       
  StringBuilder sb=new StringBuilder(str1);
  sb=sb.reverse();
str2=sb.toString();


    return str1.equals(str2);


        
    }
}
