class Solution {
    public int repeatedStringMatch(String a, String b) 
    {int ctr=0;
    StringBuilder str=new StringBuilder("");
    while(str.length()<b.length()){
            ctr++;
          str.append(a);
        }
    if(str.toString().contains(b))
    return (ctr);
     else if(str.append(a).toString().contains(b))
    return (ctr+1);
      else
     return -1;}}

     
