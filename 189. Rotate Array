class Solution {
    public void rotate(int[] nums, int k) {
        k=k%(nums.length);
        int arr[]=new int[k];
        int j=0;
        for(int i=nums.length-k;i<nums.length;i++)
        {
            arr[j]=nums[i];
            j++;

        }
        for(int i=nums.length-1;i>=k;i--)
        {
            nums[i]=nums[i-k];

        }
        for(int i=0;i<arr.length;i++){
            nums[i]=arr[i];

        }
        
        
         

        
        
        
        }
        
    
}
