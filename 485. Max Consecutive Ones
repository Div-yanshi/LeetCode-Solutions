class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int ctr=0,max=0;
    for(int i=0;i<nums.length;i++){
          if(nums[i]==1)
          {
            ctr++;
          }
          else{
            if(ctr>max){
                max=ctr;
            }ctr=0;
          }


    }
    if(nums[nums.length-1]==1)
    {
        if((ctr+1)>max)
        max=ctr;
    }
return max;
}}
