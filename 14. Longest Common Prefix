class Solution {
    public String longestCommonPrefix(String[] strs) {
    String s="";
    Arrays.sort(strs);
    int n=strs.length;
    char[] f=strs[0].toCharArray();
    char[] se=strs[n-1].toCharArray();
    for(int i=0;i<f.length;i++){
        if(f[i]!=se[i])
        
            break;
        s=s+f[i];
    } 
    return s;
    }
    }
       
