class Solution {
    public String largestOddNumber(String num) {
        String s="";
        int max=0;
        for(int i=num.length()-1;i>=0;i--)
        {
            char c=num.charAt(i);
            if((c-'0')%2!=0){
            s=num.substring(0,i+1);
            break;}

        }return s;
        }}
       
